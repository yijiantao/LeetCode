    "def searchInsert(nums, target):\n",
    "        for _index, _value in enumerate(nums):\n",
    "            if target <= _value:\n",
    "                return _index\n",
    "        return _index + 1\n",
    "nums, target = [1,3,5,6], 5\n",
    "print (searchInsert(nums = nums, target = target))"
